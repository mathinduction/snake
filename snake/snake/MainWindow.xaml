<Window x:Class="snake.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StartWindow" Height="150" Width="200" ResizeMode="NoResize" >
    <Grid>
		<Grid.RowDefinitions>
			<RowDefinition />
			<RowDefinition />
		</Grid.RowDefinitions>
		<Button Name="buttonStart" Grid.Row="0" Height="30" Width="100" Click="buttonStart_Click">Press To Start</Button>
		<Button Name="buttonEditor" Grid.Row="1" Height="30" Width="100" Click="buttonEditor_Click">Editor</Button>
	</Grid>
</Window>
